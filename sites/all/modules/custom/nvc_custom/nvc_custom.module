                $row = $start_day . "  " . $start_time . " -- " . $end_time;
            } else {
                $row = $start_day . "  " . $start_time;
            }
        } else {
            if ($start_time != $end_time) {
                $row = $start_day . "  " . $start_time . " -- " . $end_day . " Time:" . $end_time;
            } else {
                $row = $start_day . " -- " . $end_day . " " . $start_time;
            }
        }
        return $row;
    }
}


